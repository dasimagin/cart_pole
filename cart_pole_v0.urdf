<?xml version="1.0"?>
<!-- simple model of  https://myhub.autodesk360.com/ue2d4f409/g/shares/SH919a0QTf3c32634dcf8c4bfa896d40a177 -->
<robot name="cart_pole">
  <!-- materials: -->

  <material name="steel">
    <color rgba="0.75 0.75 0.75 0.75"/>
  </material>

  <material name="orange">
    <color rgba="1.0 0.82 0.00 1.0"/>
  </material>

  <material name="blue">
    <color rgba="0.00 0.74 1.0 1.0"/>
  </material>

  <!-- links: -->

  <link name="slide_bar">
    <visual>
      <geometry>
        <cylinder length="0.50" radius="0.004"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 1.5708 0"/>
      <material name="steel"/>
    </visual>
    <inertial>
      <mass value="0"/>
      <!-- ignored -->
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
  </link>

  <link name="left_border">
    <visual>
      <geometry>
        <box size="0.02 0.024 0.10"/>
      </geometry>
      <origin xyz="0 0 -0.03"/>
      <material name="orange"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.02 0.024 0.10"/>
      </geometry>
      <origin xyz="0 0 -0.03"/>
    </collision>
    <inertial>
      <mass value="0"/>
      <!-- ignored -->
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
  </link>

  <link name="right_border">
    <visual>
      <geometry>
        <box size="0.02 0.024 0.10"/>
      </geometry>
      <origin xyz="0 0 -0.03"/>
      <material name="orange"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.02 0.024 0.10"/>
      </geometry>
      <origin xyz="0 0 -0.03"/>
    </collision>
    <inertial>
      <mass value="0"/>
      <!-- ignored -->
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
  </link>

  <link name="cart">
    <visual>
      <geometry>
        <box size=" 0.05 0.025 0.07"/>"/>
      </geometry>
      <origin xyz="0 0 0"/>
      <material name="orange"/>
    </visual>
    <!-- <collision>
      <geometry>
        <box size=" 0.05 0.025 0.07"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </collision> -->
    <inertial>
      <mass value="0"/>
      <!-- ignored -->
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
  </link>

  <link name="pole">
    <visual>
      <geometry>
        <cylinder radius="0.004" length="0.3"/>
      </geometry>
      <origin xyz="0.0 0.0 -0.13"/>
      <material name="blue"/>
    </visual>
    <inertial>
      <origin xyz="0 0 -0.13"/>
      <!-- density 7800 kg/m3 -->
      <mass value="0.118"/>
      <!-- ignored -->
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
    <collision>
      <geometry>
        <cylinder radius="0.004" length="0.3"/>
      </geometry>
      <origin xyz="0.0 0.0 -0.13"/>
    </collision>
  </link>

  <!-- joints: -->

  <joint name="slide_bar_to_left_border" type="fixed">
    <axis xyz="0 0 0"/>
    <origin xyz="-0.24 0.0 0.0"/>
    <parent link="slide_bar"/>
    <child link="left_border"/>
  </joint>

  <joint name="slide_bar_to_right_border" type="fixed">
    <axis xyz="0 0 0"/>
    <origin xyz="0.24 0.0 0.0"/>
    <parent link="slide_bar"/>
    <child link="right_border"/>
  </joint>

  <joint name="slider_to_cart" type="prismatic">
    <axis xyz="1 0 0"/>
    <origin xyz="0 0 0"/>
    <parent link="slide_bar"/>
    <child link="cart"/>
    <!-- limit = (0.46 - 0.05) / 2 -->
    <limit effort="1000.0" lower="-0.205" upper="0.205" velocity="10"/>
  </joint>

  <joint name="cart_to_pole" type="continuous">
    <axis xyz="0 1 0"/>
    <origin xyz="0.0 0.04 0"/>
    <parent link="cart"/>
    <child link="pole"/>
  </joint>

</robot>
